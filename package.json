{
  "name": "babel-preset-env",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.19.5",
    "brace": "^0.11.1",
    "node-sass": "^4.13.1",
    "react": "^16.8.6",
    "react-ace": "^7.0.2",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "homepage": "https://lihautan.com/babel-preset-env",
